{
  "name": "barenode",
  "displayName": "barenode",
  "version": "0.3.0",
  "description": "Barebone node project, personal preferences.",
  "homepage": "https://larsjung.de/barenode/",
  "bugs": "https://github.com/lrsjng/barenode/issues",
  "author": "Lars Jung <lrsjng@gmail.com> (https://larsjung.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lrsjng/barenode.git"
  },
  "main": "es5/barenode",
  "scripts": {
    "lint": "eslint .",
    "test": "node -r babel-core/register test/tests.js",
    "cover": "babel-node node_modules/.bin/isparta cover --dir coverage --report text --report html test/tests.js",
    "check": "npm run -s lint && npm run -s test",
    "build": "rimraf es5 && babel -d es5 lib"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-core": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-preset-es2015": "6.9.0",
    "eslint": "2.10.2",
    "isparta": "4.0.0",
    "rimraf": "2.5.2",
    "scar": "0.10.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
